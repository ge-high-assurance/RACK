<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:sw="http://arcos.rack/SOFTWARE#"
    xmlns:sadlbasemodel="http://sadl.org/sadlbasemodel"
    xmlns:provs="http://arcos.rack/PROV-S"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:sadlimplicitmodel="http://sadl.org/sadlimplicitmodel"
    xmlns:builtinfunctions="http://sadl.org/builtinfunctions"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="http://arcos.rack/SOFTWARE">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="PROV-S"/>
    <owl:imports rdf:resource="http://sadl.org/builtinfunctions"/>
    <owl:imports rdf:resource="http://sadl.org/sadlimplicitmodel"/>
    <owl:imports rdf:resource="http://sadl.org/sadlbasemodel"/>
    <rdfs:comment xml:lang="en">This ontology was created from a SADL file 'SOFTWARE.sadl' and should not be directly edited.</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="COMPILE">
    <rdfs:comment xml:lang="en">ACTIVITY of running a a COMPILER to produce EXECUTABLEs or OBJECT_FILEs</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ACTIVITY"/>
  </owl:Class>
  <owl:Class rdf:ID="LIBRARY">
    <rdfs:comment xml:lang="en">TODO</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ENTITY"/>
  </owl:Class>
  <owl:Class rdf:ID="PACKAGE_FILE">
    <rdfs:comment xml:lang="en">a package file, e.g. Reg Hat's .rpm, Debian's .deb, etc.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ENTITY"/>
  </owl:Class>
  <owl:Class rdf:ID="CODE_GENERATOR">
    <rdfs:comment xml:lang="en">a tool that generates CODE_FILEs as part of a CODE_GEN ACTIVITY</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#AGENT"/>
  </owl:Class>
  <owl:Class rdf:ID="CODE_FILE">
    <rdfs:comment xml:lang="en">file that contains code that may be compiled and/or executed</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ENTITY"/>
  </owl:Class>
  <owl:Class rdf:ID="LANGUAGE">
    <rdfs:comment xml:lang="en">describes a programming language, possibly identifying some version of its specification</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ENTITY"/>
  </owl:Class>
  <owl:Class rdf:ID="CODE_GEN">
    <rdfs:comment xml:lang="en">ACTIVITY of running a CODE_GENERATOR to create CODE_FILEs</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ACTIVITY"/>
  </owl:Class>
  <owl:Class rdf:ID="OBJECT_FILE">
    <rdfs:comment xml:lang="en">compiled object code, not ready for execution</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ENTITY"/>
  </owl:Class>
  <owl:Class rdf:ID="BUILD">
    <rdfs:comment xml:lang="en">describes a set of ACTIVITYs (e.g. CODE_GEN, COMPILE, ...) that together form some logical building unit</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ACTIVITY"/>
  </owl:Class>
  <owl:Class rdf:ID="EXECUTABLE">
    <rdfs:comment xml:lang="en">compiled executable, ready for execution</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ENTITY"/>
  </owl:Class>
  <owl:Class rdf:ID="PACKAGER">
    <rdfs:comment xml:lang="en">a tool that packages some software together as part of a PACKAGE ACTIVITY</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#AGENT"/>
  </owl:Class>
  <owl:Class rdf:ID="COMPILER">
    <rdfs:comment xml:lang="en">a tool that compiles CODE_FILEs into ENTITYs such as OBJECT_FILE or EXECUTABLE</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#AGENT"/>
  </owl:Class>
  <owl:Class rdf:ID="PACKAGE">
    <rdfs:comment xml:lang="en">ACTIVITY of running a PACKAGER to produce a PACKAGE_FILE</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ACTIVITY"/>
  </owl:Class>
  <owl:Class rdf:ID="CONFIG_FILE">
    <rdfs:comment xml:lang="en">anything that can be considered configuration</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ENTITY"/>
  </owl:Class>
  <owl:Class rdf:ID="CODE_DEVELOPMENT">
    <rdfs:comment xml:lang="en">an ACTIVITY that produces CODE_FILEs</rdfs:comment>
    <rdfs:subClassOf rdf:resource="PROV-S#ACTIVITY"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="governedBy">
    <rdfs:subPropertyOf rdf:resource="PROV-S#used"/>
    <rdfs:range rdf:resource="PROV-S#ENTITY"/>
    <rdfs:domain rdf:resource="#CODE_DEVELOPMENT"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="satisfies">
    <rdfs:subPropertyOf rdf:resource="PROV-S#wasDerivedFrom"/>
    <rdfs:range rdf:resource="PROV-S#ENTITY"/>
    <rdfs:domain rdf:resource="#CODE_FILE"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="referenced">
    <rdfs:subPropertyOf rdf:resource="PROV-S#used"/>
    <rdfs:range rdf:resource="PROV-S#ENTITY"/>
    <rdfs:domain rdf:resource="#CODE_DEVELOPMENT"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="step">
    <rdfs:subPropertyOf rdf:resource="PROV-S#wasInformedBy"/>
    <rdfs:range rdf:resource="PROV-S#ACTIVITY"/>
    <rdfs:domain rdf:resource="#BUILD"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="language">
    <rdfs:subPropertyOf rdf:resource="PROV-S#wasDerivedFrom"/>
    <rdfs:range rdf:resource="#LANGUAGE"/>
    <rdfs:domain rdf:resource="#CODE_FILE"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="createBy">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#CODE_FILE"/>
          <owl:Class rdf:about="#CONFIG_FILE"/>
          <owl:Class rdf:about="#OBJECT_FILE"/>
          <owl:Class rdf:about="#LIBRARY"/>
          <owl:Class rdf:about="#EXECUTABLE"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="PROV-S#ACTIVITY"/>
    <rdfs:subPropertyOf rdf:resource="PROV-S#wasGeneratedBy"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="performedBy">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#CODE_GEN"/>
          <owl:Class rdf:about="#COMPILE"/>
          <owl:Class rdf:about="#PACKAGE"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="PROV-S#AGENT"/>
    <rdfs:subPropertyOf rdf:resource="PROV-S#wasAssociatedWith"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="author">
    <rdfs:subPropertyOf rdf:resource="PROV-S#wasAssociatedWith"/>
    <rdfs:range rdf:resource="PROV-S#AGENT"/>
    <rdfs:domain rdf:resource="#CODE_DEVELOPMENT"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="version">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#LANGUAGE"/>
  </owl:DatatypeProperty>
</rdf:RDF>
